{
  "name": "new-payment-ui",
  "version": "1.1.0",
  "description": "New Payment UI",
  "main": "index.js",
  "types": "./build/main.d.ts",
  "scripts": {
    "start": "webpack serve --config webpack.config.ts --mode development --env development",
    "start:prod": "webpack serve --config webpack.config.ts --mode production --env production",
    "report": "webpack --mode=production --node-env=production --profile --json > webpack-stats.json; webpack-bundle-analyzer ./webpack-stats.json",
    "watch": "webpack --watch",
    "build": "webpack --mode=production --node-env=production --config webpack.config.ts",
    "build:dev": "webpack --mode=development --node-env=development --config webpack.config.ts",
    "build:prod": "webpack --mode=production --node-env=production --config webpack.config.ts",
    "prepare": "cd .. && husky install src/.husky",
    "pre-commit": "lint-staged && npm run lint",
    "check-types": "tsc --noemit",
    "eslint": "eslint . --ext .ts",
    "lint": "npm run eslint && npm run check-types",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "cy:run": "cypress run --config-file cypress.config.ts --browser chrome",
    "cy:open": "cypress open --config-file cypress.config.ts",
    "report:text": "npx nyc report --reporter=text-summary",
    "compile": "graphql-codegen",
    "watch:apollo": "graphql-codegen -w"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prettier": "@payconstruct/prettier-config",
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
